{
	"compilerOptions": {
		"target": "es2019",
		"lib": ["es2019"],
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"strict": true,
		"types": ["node"],
		"baseUrl": "./",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true
	},
	"typeRoots": ["./node_modules/@types"]
}
